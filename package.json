{
  "name": "commerce-front-components",
  "version": "0.1.0",
  "description": "Stencil Component Starter",
  "publishConfig": {
    "access": "public"
  },
  "main": "dist/index.cjs.js",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.cjs.js",
      "default": "./dist/esm/commerce-front-components.js"
    },
    "./loader": {
      "import": "./loader/index.esm.js",
      "require": "./loader/index.cjs.js"
    },
    "./vue-component-lib": "./vue-component-lib"
  },
  "unpkg": "dist/commerce-front-components/commerce-front-components.esm.js",
  "module": "dist/index.js",
  "es2015": "dist/esm/index.mjs",
  "es2017": "dist/esm/index.mjs",
  "types": "dist/types/index.d.ts",
  "collection": "dist/collection/collection-manifest.json",
  "collection:main": "dist/collection/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-team/stencil-component-starter.git"
  },
  "files": [
    "dist/",
    "loader/"
  ],
  "scripts": {
    "build": "stencil build --docs",
    "start": "stencil build --docs --dev --watch --serve",
    "test": "stencil test --spec --e2e",
    "test.watch": "stencil test --spec --e2e --watchAll",
    "generate": "stencil generate",
    "prepare": "npm run build"
  },
  "dependencies": {
    "@splidejs/splide": "^4.1.4",
    "@stencil/core": "4.17.0",
    "@uxshop/storefront-core": "https://github.com/uxshop/storefront-core",
    "dotenv": "^16.4.7",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@stencil/sass": "^3.0.5",
    "@stencil/vue-output-target": "^0.8.8",
    "@types/jest": "^27.5.2",
    "@types/lodash": "^4.14.199",
    "@types/node": "^16.18.11",
    "@typescript-eslint/eslint-plugin": "^6.7.3",
    "@typescript-eslint/parser": "^6.7.3",
    "branch-name-lint": "^2.1.1",
    "eslint": "^8.50.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-config-standard": "^17.1.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jest": "^27.4.0",
    "eslint-plugin-n": "^16.1.0",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-promise": "^6.1.1",
    "git-commit-msg-linter": "^5.0.4",
    "husky": "^8.0.3",
    "jest": "^27.5.1",
    "jest-cli": "^27.5.1",
    "lint-staged": "^14.0.1",
    "prettier": "3.0.3",
    "puppeteer": "21.1.1",
    "stylelint-config-prettier": "^9.0.5",
    "typescript": "^5.2.2",
    "vue": "^3.5.13"
  },
  "license": "MIT"
}
